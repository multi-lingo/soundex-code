{
  "name": "soundex-code",
  "version": "1.0.2",
  "description": "Soundex phonetic algorithm",
  "license": "MIT",
  "keywords": [
    "natural",
    "language",
    "phonetics",
    "soundex",
    "cli",
    "bin"
  ],
  "homepage": "https://words.github.io/soundex-code/",
  "repository": "words/soundex-code",
  "bugs": "https://github.com/words/soundex-code/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js",
    "cli.js"
  ],
  "bin": "cli.js",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^16.0.0",
    "esmangle": "^1.0.0",
    "execa": "^1.0.0",
    "nyc": "^13.0.0",
    "remark-cli": "^5.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "tape": "^4.4.0",
    "xo": "^0.22.0"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build-bundle": "browserify index.js -s soundexCode > soundex-code.js",
    "build-mangle": "esmangle soundex-code.js > soundex-code.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "no-multi-assign": "off"
    },
    "ignores": [
      "soundex-code.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
